<?xml version="1.0" encoding="UTF-8"?>
<openerp>
    <data>
        <!-- Magazine Form -->
        <record model="ir.ui.view" id="product_magazine_muller_view">
            <field name="name">product.magazine.muller.view</field>
            <field name="model">product.template</field>
            <field name="inherit_id" ref="product_magazine.product_magazine_view"/>
            <field name="arch" type="xml">
                <xpath expr="//field[@name='journal_id']" position="after">
                    <field name="used_for_muller" />
                </xpath>
            </field>
        </record>
    </data>
</openerp>